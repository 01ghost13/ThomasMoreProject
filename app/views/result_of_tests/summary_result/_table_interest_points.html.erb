<table class="table table-hover">
  <thead>
  <h1><%= tf('entities.result_of_tests.earned_points') %></h1>
  <tr>
    <td><h4><%= tf('entities.interests.interest') %></h4></td>
    <td><h4><%= tf('entities.result_of_tests.summary') %></h4></td>
  </tr>
  </thead>
  <% interest_points.each do |interest| %>
    <tr>
      <td><h4><small><%= interest.this.name %></small></h4></td>
      <td><h4><small><%= interest.total_weight %>/<%= interest.max_weight %></small></h4></td>
    </tr>
  <% end %>
</table>
