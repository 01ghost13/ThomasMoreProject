<div class="row">
	<%= render "shared/profile" %>
</div>
<% if is_super? and current_user.id != params[:id].to_i %>
	<div class="row">
		<div class="btn-group col-sm-4" role="group" aria-label="...">
			<%= link_to("Edit", edit_administrator_path(params[:id]), class: "btn btn-warning") %>
			<%= link_to("Delete",@user, method: :delete, data: { confirm: "You sure?" }, class: "btn btn-danger") %>
		</div>
	</div>
<% end %>


